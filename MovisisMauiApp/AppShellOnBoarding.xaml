<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="MovisisMauiApp.AppShellOnBoarding"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:onboarding="clr-namespace:MovisisMauiApp.Pages"
    Shell.BackgroundColor="{DynamicResource MainWrapperBackgroundColor}"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Shell.PresentationMode="Animated">

    <ShellContent
        Title="Start"
        ContentTemplate="{DataTemplate onboarding:StartPage}"
        Icon="home_empty.png"
        Route="StartPage" />

</Shell> 